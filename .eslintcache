assName={styles.container}>\r\n        {containment.map((i, ind) => {\r\n          return (\r\n            <ModalListItem\r\n              name={i.name}\r\n              cost={i.cost}\r\n              qty={i.qty}\r\n              aTotal={i.aTotal}\r\n              key={ind}\r\n            />\r\n          );\r\n        })}\r\n        <FinalSum billTotal={billTotal}></FinalSum>\r\n      </div>\r\n    </section>\r\n  );\r\n}\r\n\r\nexport default ModalFace;\r\n","C:\\Users\\jason\\Desktop\\learn-projects\\food_app\\src\\components\\Mission.js",[],"C:\\Users\\jason\\Desktop\\learn-projects\\food_app\\src\\components\\Header.js",["98"],"import React from \"react\";\r\nimport styles from \"./Header.module.scss\";\r\nimport { useCustomHook } from \"../GlobalContext\";\r\n\r\nfunction Header() {\r\n  //% Track the number of cart items based on the meal quantities inside orderState\r\n  //% That's the useReducer state object, which we manage in our Context API file\r\n  const { orderState, activateModal } = useCustomHook();\r\n  const precartMealSum = function () {\r\n    let mealsBeforeCart = 0;\r\n    for (let key in orderState) {\r\n      mealsBeforeCart = mealsBeforeCart + orderState[key];\r\n    }\r\n    return mealsBeforeCart;\r\n  };\r\n\r\n  return (\r\n    <section className={styles.navbar}>\r\n      <h1>React Meals</h1>\r\n      <div className={styles.button} onClick={activateModal}>\r\n        <img src=\"data:image/svg+xml;base64,\" />\r\n        <span className={styles.yourCart}>Items in Cart:</span>\r\n        <span className={styles.qty}>{precartMealSum()}</span>\r\n      </div>\r\n    </section>\r\n  );\r\n}\r\n\r\nexport default Header;\r\n","C:\\Users\\jason\\Desktop\\learn-projects\\food_app\\src\\components\\Menu\\ListItem.js",[],"C:\\Users\\jason\\Desktop\\learn-projects\\food_app\\src\\components\\Modal\\ModalListItem.js",[],"C:\\Users\\jason\\Desktop\\learn-projects\\food_app\\src\\components\\Modal\\FinalSum.js",[],"C:\\Users\\jason\\Desktop\\learn-pro