ment\r\n      const name = key;\r\n      const cost = meals[key].price;\r\n      const qty = currentOrder[key];\r\n      const aTotal = modalFace_helpers_2B3.round(\r\n        currentOrder[key] * Number(meals[key].price.slice(1)),\r\n        2\r\n      );\r\n      containment.push({\r\n        name: name,\r\n        cost: cost,\r\n        qty: qty,\r\n        aTotal: aTotal,\r\n      });\r\n    }\r\n  }\r\n  console.log(containment);\r\n  //% Calculate the sum of all aTotal's inside the containment array\r\n  const billTotal = containment.reduce((acc, currVal) => {\r\n    return acc + currVal.aTotal;\r\n  }, 0);\r\n\r\n  return (\r\n    <section className={styles.overlay}>\r\n      <div className={styles.container}>\r\n        {containment.map((i, ind) => {\r\n          return (\r\n            <ModalListItem\r\n              name={i.name}\r\n              cost={i.cost}\r\n              qty={i.qty}\r\n              aTotal={i.aTotal}\r\n              key={ind}\r\n            />\r\n          );\r\n        })}\r\n        <FinalSum billTotal={billTotal}></FinalSum>\r\n      </div>\r\n    </section>\r\n  );\r\n}\r\n\r\nexport default ModalFace;\r\n","C:\\Users\\jason\\Desktop\\learn-projects\\food_app\\src\\components\\Mission.js",[],"C:\\Users\\jason\\Desktop\\learn-projects\\food_app\\src\\components\\Header.js",["83"],"import React from \"react\";\r\nimport styles from \"./Header.module.scss\";\r\nimport { useCustomHook } from \"../GlobalContext\";\r\n\r\nfunction Header() {\r\n  //% Track the number of cart items based on the meal quantities inside orderState\r\n  //% That's the useReducer state object, which we manage in our Context API file\r\n  const { orderState, activateModal } = useCustomHook();\r\n  const precartMealSum = function () {\r\n    let mealsBeforeCart = 0;\r\n    for (let key in orderState) {\r\n      mealsBeforeCart = mealsBeforeCart + orderState[key];\r\n    }\r\n    return mealsBeforeCart;\r\n  };\r\n\r\n  return (\r\n    <section className={style